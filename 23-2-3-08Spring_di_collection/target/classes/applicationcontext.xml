<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="bookDao" class="com.HM.dao.impl.BookDaoImpl">
    <property name="array">
        <array>
            <value>100</value>
            <value>200</value>
            <value>300</value>
        </array>
    </property>

    <property name="list">
        <list>
            <value>itcase</value>
            <value>100</value>
            <value>200</value>
            <value>300</value>
        </list>
    </property>

    <property name="set">
        <set>
            <value>200</value>
            <value>itcase</value>
            <value>lisi</value>
            <value>lisi</value>
        </set>
    </property>
    <property name="map">
        <map>
            <entry key="country" value="china"/>
            <entry key="province" value="hebei"/>
            <entry key="city" value="baoding"/>
        </map>
    </property>
    <property name="properties">
        <props>
            <prop key="country">china</prop>
            <prop key="province">hebei</prop>
            <prop key="city">baoding</prop>
        </props>
    </property>
</bean>
</beans>